def destroy_file(file_path):
    """Destroys the specified file by overwriting its contents before deletion."""
    if os.path.isfile(file_path):
        try:
            # Overwrite the file with zeros
            with open(file_path, 'r+b') as f:
                length = os.path.getsize(file_path)
                f.write(b'\x00' * length)
            # Delete the file
            os.remove(file_path)
            print(f"File '{file_path}' has been destroyed.")
        except Exception as e:
            print(f"An error occurred while destroying the file: {e}")
    else:
        print(f"The file '{file_path}' does not exist.")

